{
    "name": "boilerplate-site",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "node ./build/bin/www.js",
        "heroku": "NODE_ENV=production && npm-run-all serverprod",
        "serverdev": "./node_modules/.bin/babel-node ./src/bin/www.js",
        "serverprod": "node ./build/bin/www.js",
        "prod": "npm-run-all clean buildserver compileprod serverprod",
        "buildserver": "./node_modules/.bin/babel ./src --out-dir build",
        "compiledev": "webpack --config webpack.config.js",
        "compileprod": "webpack --config webpack.prod.js",
        "clean": "rimraf build public/javascripts/ public/stylesheets/",
        "watch": "nodemon --config nodemon-config.json",
        "package": "npm-run-all clean buildserver compileprod",
        "build": "npm run package",
        "package:docker": "npm ci && npm run package",
        "docker:build": "docker build ."
    },
    "dependencies": {
        "@babel/runtime": "^7.8.0",
        "aws-sdk": "^2.601.0",
        "axios": "^0.19.1",
        "chalk": "^3.0.0",
        "connect-ensure-login": "^0.1.1",
        "cookie-parser": "~1.4.3",
        "debug": "~4.1.1",
        "express": "~4.17.1",
        "express-session": "^1.17.0",
        "helmet": "^3.21.2",
        "howler": "^2.1.3",
        "http-errors": "~1.7.3",
        "immutable": "^4.0.0-rc.12",
        "moment": "^2.24.0",
        "mongodb": "^3.5.2",
        "mongoose": "^5.8.7",
        "morgan": "~1.9.0",
        "multer": "^1.4.2",
        "multer-s3": "^2.9.0",
        "nodemon": "^2.0.2",
        "npm": "^6.13.6",
        "npm-run-all": "^4.1.5",
        "passport": "^0.4.1",
        "passport-google-oauth20": "^2.0.0",
        "paypal-rest-sdk": "^1.8.1",
        "pm2": "^4.2.3",
        "pug": "2.0.4",
        "razorpay": "^2.0.6",
        "react": "^16.11.0",
        "react-dom": "^16.11.0",
        "react-faq-component": "^1.0.3",
        "react-howler-player": "^1.0.6",
        "react-quill": "^1.3.3",
        "react-tagsinput": "^3.19.0",
        "react-textinput-field": "^1.0.0",
        "rimraf": "^3.0.0",
        "uuid": "^3.3.3",
        "workbox-webpack-plugin": "^5.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.0",
        "@babel/core": "^7.8.0",
        "@babel/node": "^7.8.0",
        "@babel/plugin-proposal-class-properties": "^7.8.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.8.0",
        "@babel/plugin-transform-runtime": "^7.8.0",
        "@babel/preset-env": "^7.8.0",
        "@babel/preset-react": "^7.8.0",
        "babel-loader": "^8.0.6",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^3.4.2",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.13.0",
        "sass-loader": "^8.0.2",
        "terser-webpack-plugin": "^2.3.2",
        "url-loader": "^3.0.0",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-merge": "^4.2.2"
    }
}
