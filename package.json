{
  "name": "transcribe-site",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server:dev": "babel-node ./src/bin/www",
    "server:prod": "node ./build/bin/www",
    "dev": "SET NODE_ENV=development && npm-run-all build server:dev",
    "prod": "SET NODE_ENV=production && npm-run-all clean build server:prod",
    "build": "babel ./src --out-dir build",
    "clean": "rimraf build",
    "watch": "nodemon"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "npm-run-all": "^4.1.5",
    "pug": "2.0.0-beta11",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "nodemon": "^1.19.1"
  }
}
